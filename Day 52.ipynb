     "output_type": "stream",
     "text": [
      "0\n",
      "1\n",
      "4\n",
      "16\n",
      "25\n",
      "49\n",
      "64\n",
      "81\n",
      "100\n"
     ]
    }
   ],
   "source": [
    "i = 0\n",
    "while i<=10:\n",
    "    if i ==3 or i==6:\n",
    "        i=i+1\n",
    "        continue\n",
    "    else:\n",
    "        print(i*i)\n",
    "        i=i+1\n",
    "    "
