   "source": [
    "write a program to print the square of the numbers between 1 to 10 except 3 and 6"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "ecad6fe1",
   "metadata": {},
   "outputs": [
    {
