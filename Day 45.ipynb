   "source": [
    "Write a program to find a leap year?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "21b63da9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "enter a year:2000\n",
      "2000 is a leap year\n"
     ]
    }
   ],
   "source": [
    "year  = int(input('enter a year:'))\n",
    "if year % 4 == 0 and year % 400==0 or year % 100!=0:\n",
    "    print(year,'is a leap year')\n",
    "else:\n",
    "    print(year,'not a leap year')"
   ]
  }
