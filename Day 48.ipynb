   ],
   "source": [
    "def func(n):\n",
    "    if len(n)==3:\n",
    "        print(n,\" is  a three digit number\")\n",
    "    else:\n",
    "        print(n,'is not a three digit number')\n",
    "        \n",
    "func('100')"
   ]
